config sim1 'sim1'
        option dialnumber '*99#'
        option reconnect '5'
        option ltebandval '1a0000800d5'
        option bandval 'ffff'
        option auth_mode 'none'
        option service 'auto'
        option pdptype '1'
        option proto '3g'
        option ifname '3g-ppp'
        option device '/dev/modem_data'
        option method 'nat'
        option auto_apn '0'
        option force_apn '-1'
        option apn 'apn2.greenflux.tele2.m2m'
        option mtu '1500'

config sim2 'sim2'
        option reconnect '5'
        option ltebandval '1a0000800d5'
        option bandval 'ffff'
        option auto_apn '1'
        option ifname 'wwan0'
        option proto 'qmi2'
        option service 'auto'
        option pdptype '1'

config interface 'ppp'
        option demand '10'

config option 'option'

config rules 'rules'

config bands 'bands'
        option auto_sim1 'enable'
        option auto_sim2 'enable'

config simcard 'simcard'
        option default 'sim1'
